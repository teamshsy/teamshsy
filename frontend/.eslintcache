mes/Seagate/react/BradTUdemy/2020c-cybs-store-tester1/frontend/src/components/Loader.js",[],"/Volumes/Seagate/react/BradTUdemy/2020c-cybs-store-tester1/frontend/src/components/Message.js",[],"/Volumes/Seagate/react/BradTUdemy/2020c-cybs-store-tester1/frontend/src/screens/CartScreen.js",[],"/Volumes/Seagate/react/BradTUdemy/2020c-cybs-store-tester1/frontend/src/reducers/cartReducers.js",[],"/Volumes/Seagate/react/BradTUdemy/2020c-cybs-store-tester1/frontend/src/actions/cartActions.js",[],"/Volumes/Seagate/react/BradTUdemy/2020c-cybs-store-tester1/frontend/src/screens/BillingScreen.js",["95","96","97","98"],"import React, { useState, useEffect } from 'react'\nimport { Link } from 'react-router-dom'\nimport { Form, Button, Row, Col } from 'react-bootstrap'\nimport { useDispatch, useSelector } from 'react-redux'\nimport FormContainer from '../components/FormContainer'\nimport CheckoutSteps from '../components/CheckoutSteps'\nimport { saveBillingAddress } from '../actions/cartActions'\n\nconst BillingScreen = ({ history }) => {\n\n  const cart = useSelector(state => state.cart)\n  const { billingAddress } = cart\n  \n  const [address, setAddress] = useState(billingAddress.address)\n  const [city, setCity] = useState(billingAddress.city)\n  const [postalCode, setPostalCode] = useState(billingAddress.postalCode)\n  const [country, setCountry] = useState(billingAddress.country)\n\n  const dispatch = useDispatch()\n\n  const submitHandler = (e) => {\n    e.preventDefault()\n    dispatch(saveBillingAddress({ address, city, postalCode, country}))\n    history.push('/payment')\n  }\n  \n  return (\n    <FormContainer>\n      <CheckoutSteps step1 step2 />\n      <h1>Billing</h1>\n      <Form onSubmit={submitHandler}>\n\n        <Form.Group controlId='address'>\n          <Form.Label>Address</Form.Label>\n          <Form.Control type='text' placeholder='Enter address' value={address} onChange={(e) => setAddress(e.target.value)} required></Form.Control>\n        </Form.Group>\n\n        <Form.Group controlId='city'>\n          <Form.Label>City</Form.Label>\n          <Form.Control type='text' placeholder='Enter city' value={city} onChange={(e) => setCity(e.target.value)} required></Form.Control>\n        </Form.Group>\n\n        <Form.Group controlId='postalCode'>\n          <Form.Label>Postal Code</Form.Label>\n          <Form.Control type='text' placeholder='Postal Code' value={postalCode} onChange={(e) => setPostalCode(e.target.value)} required></Form.Control>\n        </Form.Group>\n\n        <Form.Group controlId='country'>\n          <Form.Label>Country</Form.Label>\n          <Form.Control type='text' placeholder='country' value={country} onChange={(e)=> setCountry(e.target.value)} required></Form.Control>\n        </Form.Group>\n        \n        <Button type='submit' varient='primary'>Continue</Button>\n      \n      </Form>\n\n\n    </FormContainer>\n  )\n}\n\nexport default BillingScreen\n","/Volumes/Seagate/react/BradTUdemy/2020c-cybs-store-tester1/frontend/src/components/FormContainer.js",[],"/Volumes/Seagate/react/BradTUdemy/2020c-cybs-store-tester1/frontend/src/components/CheckoutSteps.js",[],"/Volumes/Seagate/react/BradTUdemy/2020c-cybs-store-tester1/frontend/src/screens/PaymentScreen.js",[],"/Volumes/Seagate/react/BradTUdemy/2020c-cybs-store-tester1/frontend/src/screens/PlaceOrderScreen.js",["99"],"import React from 'react'\nimport { Link } from 'react-router-dom'\nimport { Button, Row, Col, ListGroup, Image, Card } from 'react-bootstrap'\nimport { useDispatch, useSelector } from 'react-redux'\nimport Message from '../components/Message'\nimport CheckoutSteps from '../components/CheckoutSteps'\n\nconst PlaceOrderScreen = () => {\n\n  //grab item from cart from state:cart\n  const cart = useSelector(state => state.cart)\n\n\n  const placeOrderHandler = () => {\n    console.log('order placed')\n  }\n\n  //calculate prices\n  cart.itemsPrice = cart.cartItems.reduce((acc, item) => acc + item.price * item.qty, 0).toFixed(2)\n\n\n\n  // const dispatch = dispatch()\n\n  return (\n    <div>\n      <CheckoutSteps Step1 Step2 Step3 />\n      <Row>\n        <Col md={8}>\n          <ListGroup variant=\"flush\">\n            <ListGroup.Item>\n              <h2>Billing</h2>\n              <p>\n                <strong>Address:</strong>\n                {cart.billingAddress.address}, {' '}\n                {cart.billingAddress.city}, {' '}\n                {cart.billingAddress.postalCode}, {' '}\n                {cart.billingAddress.country}\n              </p>  \n            </ListGroup.Item>\n            <ListGroup.Item>\n              <h2>Payment Method</h2>\n              <strong>method: </strong> \n              {cart.paymentMethod}\n            </ListGroup.Item>\n\n            <ListGroup.Item>\n              <h2>Order Items</h2>\n              {cart.cartItems.length === 0 ? <Message>Your Cart is empty</Message> : (\n                <ListGroup variant='flush'>\n                  {cart.cartItems.map((item, index) => (\n                    <ListGroup.Item key={index}>\n                      <Row>\n                        <Col md={1}>\n                          <Image src={item.image} alt={item.name} fluid rounded />\n                        </Col>\n                        <Col>\n                          <Link to={`/product/${item.product}`}>\n                            {item.name}\n                          </Link>\n                        </Col>\n                        <Col md={4}>\n                          {item.qty} x ${item.price} = ${Number(item.qty * item.price).toFixed(2)}\n                        </Col>\n                      </Row>\n                    </ListGroup.Item>\n                  ))}\n                </ListGroup>\n              )}\n            </ListGroup.Item>\n\n          </ListGroup>\n        </Col>\n        <Col md={4}>\n          <Card>\n            <ListGroup variant='flush'>\n              <ListGroup.Item>\n                <h2>Summary</h2>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Row>\n                  <Col>Total</Col>\n                  <Col>${cart.itemsPrice}</Col>\n                </Row>\n              </ListGroup.Item>\n              <ListGroup.Item>\n                <Button type='button' className='btn-block' disabled={cart.cartItems === 0} onClick={placeOrderHandler}>Place Order</Button>\n              </ListGroup.Item>\n            </ListGroup>\n          </Card>\n        </Col>\n\n      </Row>\n    </div>\n  )\n}\n\nexport default PlaceOrderScreen",{"ruleId":"100","replacedBy":"101"},{"ruleId":"102","replacedBy":"103"},{"ruleId":"104","severity":1,"message":"105","line":1,"column":27,"nodeType":"106","messageId":"107","endLine":1,"endColumn":36},{"ruleId":"104","severity":1,"message":"108","line":2,"column":10,"nodeType":"106","messageId":"107","endLine":2,"endColumn":14},{"ruleId":"104","severity":1,"message":"109","line":3,"column":24,"nodeType":"106","messageId":"107","endLine":3,"endColumn":27},{"ruleId":"104","severity":1,"message":"110","line":3,"column":29,"nodeType":"106","messageId":"107","endLine":3,"endColumn":32},{"ruleId":"104","severity":1,"message":"111","line":4,"column":10,"nodeType":"106","messageId":"107","endLine":4,"endColumn":21},"no-native-reassign",["112"],"no-negated-in-lhs",["113"],"no-unused-vars","'useEffect' is defined but never used.","Identifier","unusedVar","'Link' is defined but never used.","'Row' is defined but never used.","'Col' is defined but never used.","'useDispatch' is defined but never used.","no-global-assign","no-unsafe-negation"]